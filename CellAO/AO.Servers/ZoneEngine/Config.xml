<?xml version="1.0" encoding="utf-8"?>
<Config>
  <!-- Ports for the Game Engines -->
  <ListenIP>0.0.0.0</ListenIP>
  <ChatIP>127.0.0.1</ChatIP>
  <ZoneIP>127.0.0.1</ZoneIP>
  <ChatPort>7012</ChatPort>
  <ZonePort>7501</ZonePort>
  <LoginPort>7500</LoginPort>
  <CommPort>6996</CommPort>
  <!-- Log Area -->
  <SqlLog>true</SqlLog>
  <LogChat>false</LogChat>
  <!-- Server MOTD -->
  <!-- {0} will be replaced with Cellao Name and Version eg Yalm 1.3.2.267 -->
  <Motd>Welcome to CellAO {0}, you Visit our Forums at http://www.aocell.co</Motd>
  <!-- Set this to true if you want to use encrypted passwords with your server you will need to supply each user the launcher we have created -->
  <UsePassword>false</UsePassword>
  <!-- Choose your SQL Databse Type here, MySql, MsSql, PostgreSQL-->
  <SQLType>MySql</SQLType>
  <!-- Sql Connection Setup Area -->
  <MysqlConnection>Database=cellao;Data Source=localhost;User ID=cellaodbuser;Password=password</MysqlConnection>
  <MsSqlConnection>Data Source=localhost;Inital Catalog=cellao;UserID=cellaodbuser;Password=password;Trusted_Connection=false</MsSqlConnection>
  <PostgreConnection>Server=localhost;User Id=cellaodbuser;Password=password;Database=cellao</PostgreConnection>
  <!-- This area is for Locale Settings, en = english, gr = german, ro = Romanian more languages will be supported later, Default = en-->
  <Locale>en</Locale>
  <!-- Web Engine Configuration -->
  <serverName>localhost</serverName>
  <Host>
    <Dir>C:\EugeneServer</Dir>
    <Port>5555</Port>
  </Host>
  <php>
    <Path>c:\php</Path>
  </php>
  <Forbidden>
    <Path>C:\EugeneServer\bin\</Path>
  </Forbidden>
  <Default>
    <File>Index.html</File>
    <File>Default.html</File>
    <File>Default.aspx</File>
    <File>Home.aspx</File>
  </Default>
  <Mime>
    <Values>
      <Ext>.htm</Ext>
      <Type>text/html</Type>
    </Values>
    <Values>
      <Ext>.html</Ext>
      <Type>text/html</Type>
    </Values>
    <Values>
      <Ext>.css</Ext>
      <Type>text/css</Type>
    </Values>
    <Values>
      <Ext>.js</Ext>
      <Type>text/javascript</Type>
    </Values>
    <Values>
      <Ext>.txt</Ext>
      <Type>text/plain</Type>
    </Values>
    <Values>
      <Ext>.xml</Ext>
      <Type>text/xml</Type>
    </Values>
    <Values>
      <Ext>.aspx</Ext>
      <Type>text/html</Type>
    </Values>
    <Values>
      <Ext>.php</Ext>
      <Type>text/html</Type>
    </Values>
    <Values>
      <Ext>.exe</Ext>
      <Type>text/html</Type>
    </Values>
    <Values>
      <Ext>.swf</Ext>
      <Type>application/x-shockwave-flash</Type>
    </Values>
    <Values>
      <Ext>.gif</Ext>
      <Type>image/gif</Type>
    </Values>
    <Values>
      <Ext>.jpg</Ext>
      <Type>image/jpg</Type>
    </Values>
    <Values>
      <Ext>.png</Ext>
      <Type>image/x-png</Type>
    </Values>
    <Values>
      <Ext>.tiff</Ext>
      <Type>image/tiff</Type>
    </Values>
    <Values>
      <Ext>.pdf</Ext>
      <Type>application/pdf</Type>
    </Values>
    <Values>
      <Ext>.zip</Ext>
      <Type>application/zip</Type>
    </Values>
    <Values>
      <Ext>.rar</Ext>
      <Type>application/rar</Type>
    </Values>
    <Values>
      <Ext>.mpeg</Ext>
      <Type>video/mpeg</Type>
    </Values>
    <Values>
      <Ext>.mpg</Ext>
      <Type>video/mpeg</Type>
    </Values>
    <Values>
      <Ext>.mpg</Ext>
      <Type>video/mpeg</Type>
    </Values>
    <Values>
      <Ext>.wmv</Ext>
      <Type>video/x-ms-wmv</Type>
    </Values>
    <Values>
      <Ext>.avi</Ext>
      <Type>video/x-msvideo</Type>
    </Values>
  </Mime>
</Config>